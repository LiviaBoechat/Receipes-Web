{
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "db:reset": "npx tsc && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "build": "tsc --project tsconfig.json",
    "prestart": "npm run build",
    "start": "node ./dist/server.js",
    "dev": "tsnd --watch \"./src/**\" --transpile-only ./src/server.ts",
    "lint": "eslint ./src --ext .ts",
    "generate:types": "npx supabase gen types typescript --project-id rtpoziwmyvjihhsfomym --schema public > ./src/database.types.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.39.2",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "docker": "1.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "9.0.1",
    "mysql2": "2.3.3",
    "node": "16.14.0",
    "sequelize": "6.25.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jsonwebtoken": "8.5.6",
    "@types/node": "16.11.7",
    "@types/sequelize": "4.28.10",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "eslint": "7.32.0",
    "nodemon": "2.0.15",
    "sequelize-cli": "6.3.0",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.4.4"
  }
}
